{
    "name": "vsc-fennel",
    "displayName": "Fennel",
    "description": "VS Code Fennel support",
    "version": "0.1.5",
    "publisher": "kongeor",
    "scripts": {
        "grammar-compile:watch": "ts-node --project script/compile-grammar/tsconfig.json --transpile-only script/compile-grammar"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/kongeor/vsc-fennel"
    },
    "contributes": {
        "languages": [
            {
                "id": "fennel",
                "aliases": [
                    "Fennel",
                    "fennel"
                ],
                "extensions": [
                    ".fnl"
                ],
                "configuration": "./fennel.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fennel",
                "scopeName": "source.fennel",
                "path": "./syntaxes/fennel.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "@tsconfig/node14": "^1.0.0",
        "@types/chalk": "^2.2.0",
        "@types/debug": "^4.1.5",
        "@types/js-yaml": "^4.0.1",
        "@types/node": "14",
        "@types/plist": "^3.0.2",
        "@types/yargs": "^17.0.0",
        "chalk": "^4.1.1",
        "chokidar": "^3.5.1",
        "debug": "^4.3.1",
        "js-yaml": "^4.1.0",
        "plist": "^3.0.2",
        "tsdef": "^0.0.14",
        "vscode-tmgrammar-test": "^0.0.11",
        "yargs": "^17.0.1"
    }
}
